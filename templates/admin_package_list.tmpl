<h2>Packages</h2>

<%LOOP PAGES %>
    <%IF PAGE %>
        <a href="<%= __SCRIPT__ %>/packages/list/<%= PAGE %>"><%= PAGE %></a>
    <%ELSE %>
        ...
    <%/IF %>
<%/LOOP %>

<table id="package_list">
  <tr>
    <th>PackageName</th>
    <th>Version</th>
    <th>Datum</th>
    <th>Maintainer</th>
    <th>Co-Maintainer</th>
    <th>Aktion</th>
  </tr>
  <%LOOP PACKAGES %>
  <tr>
    <td><%= NAME %></td>
    <td><%= DATE %></td>
    <td><%= VERSION %></td>
    <td><%= AUTHOR %></td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
  <%/LOOP %>
</table>